.new
  %h1= @roster.section_name
  %ul
    %li
      %strong Meet Time:
      =@roster.meet_day
      at
      =@roster.meet_time
    %li
      %strong Location:
      =@roster.location

  = link_to "Export Roster", export_roster_path
  .grid
    %div.row.header
      %div.cell.header
        Name
      %div.cell.header
        Email Address
      %div.cell.header
        Uid
      - @roster.evals.each do |eval|
        %div.cell.header
          = link_to "#{eval.title}", eval_path(eval)

    - @roster.students.each do |student|
      .row
        .cell
          = first_last(student)
        .cell
          = student.email
        .cell
          = student.uid
        -student.evals.each do |eval|
          .cell
            = link_to "#{eval.title}", new_student_grade_path(student, eval_id: eval.id)
%table.table-minimal
  %thead
    %tr
      %th Header Cell 1
      %th Header Cell 2
      %th Header Cell 3
  %tbody
    %tr
      %td Row 1 Cell 1
      %td Row 1 Cell 2
      %td
        %button Confirm

    %tr
      %td Row 2 Cell 1
      %td Row 2 Cell 2
      %td
        %button Confirm

    %tr
      %td Row 3 Cell 1
      %td Row 3 Cell 2
      %td
        %button Confirm

    %tr
      %td Row 4 Cell 1
      %td Row 4 Cell 2
      %td
        %button Confirm








